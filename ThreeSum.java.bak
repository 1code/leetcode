import java.util.Arrays;
import java.util.HashSet;

public class Solution {
    public ArrayList<ArrayList<Integer>> threeSum(int[] num) {
        // Start typing your Java solution below
        // DO NOT write main() function
        Arrays.sort(num);
    			HashSet<Integer> unique1 = new HashSet<Integer>();
				HashSet<Integer> unique2 = new HashSet<Integer>();
				HashSet<Integer> unique3 = new HashSet<Integer>();
				
				ArrayList<ArrayList<Integer>> results = new ArrayList<ArrayList<Integer>>();
				
				int size = num.length;
				for(int i = 0; i < size; i++){
					if(!unique1.contains(num[i])){
						unique1.add(num[i]);
                        unique2 = new HashSet<Integer>();
						for(int j = i + 1; j < size; j++){
                            
							if(!unique2.contains(num[j])){
								unique2.add(num[j]);
                                unique3 = new HashSet<Integer>();
								for(int k = size - 1; k > j; k--){
									if(!unique3.contains(num[k])){
										unique3.add(num[k]);
										int threeSum = num[i] + num[j] + num[k];
										if(threeSum == 0){
											ArrayList<Integer> oneResult = new ArrayList<Integer>();
											oneResult.add(num[i]);
											oneResult.add(num[j]);
											oneResult.add(num[k]);
											results.add(oneResult);
                                            break;
										}
									}
								}
							}
						}
					}
				}
				return results;
    }
}